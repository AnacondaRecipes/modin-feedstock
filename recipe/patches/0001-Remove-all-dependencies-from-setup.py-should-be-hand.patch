From 725b5ab7e0c9285b592138773be3503b4e7885b8 Mon Sep 17 00:00:00 2001
From: Vasilij Litvinov <vasilij.n.litvinov@intel.com>
Date: Tue, 13 Jul 2021 11:58:27 +0300
Subject: [PATCH] Remove all dependencies from setup.py - should be handled by
 meta.yaml

Signed-off-by: Vasilij Litvinov <vasilij.n.litvinov@intel.com>
---
 setup.py | 10 +++++-----
 1 file changed, 5 insertions(+), 5 deletions(-)

diff --git a/setup.py b/setup.py
index 85dcc040..d5713d25 100644
--- a/setup.py
+++ b/setup.py
@@ -37,10 +37,10 @@ class ModinDistribution(Distribution):
         return False
 
 
-dask_deps = ["dask>=2.22.0", "distributed>=2.22.0"]
-ray_deps = ["ray[default]>=1.4.0", "pyarrow>=1.0"]
-remote_deps = ["rpyc==4.1.5", "cloudpickle", "boto3==1.4.8"]
-spreadsheet_deps = ["modin-spreadsheet>=0.1.0"]
+dask_deps = []
+ray_deps = []
+remote_deps = []
+spreadsheet_deps = []
 sql_deps = ["dfsql>=0.2.6"]
 all_deps = dask_deps + ray_deps + remote_deps + spreadsheet_deps
 
@@ -60,7 +60,7 @@ setup(
     url="https://github.com/modin-project/modin",
     long_description=long_description,
     long_description_content_type="text/markdown",
-    install_requires=["pandas==1.3.0", "packaging", "numpy>=1.16.5"],
+    install_requires=[],
     extras_require={
         # can be installed by pip install modin[dask]
         "dask": dask_deps,
-- 
